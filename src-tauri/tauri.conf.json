{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "markor",
	"version": "0.2.0",
	"identifier": "com.duchiru.markor",
	"build": {
		"beforeDevCommand": "pnpm ui:dev",
		"devUrl": "http://localhost:3108",
		"beforeBuildCommand": "pnpm ui:build",
		"frontendDist": "../dist"
	},
	"app": {
		"macOSPrivateApi": true,
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
